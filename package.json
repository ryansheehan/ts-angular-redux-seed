{
  "name": "ts-angular-redux",
  "version": "1.0.0",
  "description": "seed project using typescript, angular, redux, and systemjs",
  "keywords": [
    "angular",
    "redux",
    "seed",
    "systemjs",
    "typescript"
  ],
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\"",
    "build": "tsc",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "postinstall": "typings install",
    "typings": "typings",
    "typings:version": "typings --version"
  },
  "author": "Ryan Sheehan <rsheehan@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryansheehan/ts-angular-redux-seed.git"
  },
  "private": true,
  "dependencies": {
    "angular": "^1.5.7",
    "angular-animate": "^1.5.7",
    "angular-aria": "^1.5.7",
    "angular-material": "^1.1.0-rc.5",
    "angular-ui-router": "^0.3.1",
    "es6-promise": "^3.2.1",
    "immutable": "^3.8.1",
    "moment": "^2.14.1",
    "ng-redux": "^3.3.5-beta.1",
    "normalize.css": "^4.2.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "redux-ui-router": "^0.5.2",
    "systemjs": "^0.19.31",
    "ui-router-extras": "^0.1.2"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "systemjs-builder": "^0.15.23",
    "typescript": "^1.8.10",
    "typings": "^1.3.1"
